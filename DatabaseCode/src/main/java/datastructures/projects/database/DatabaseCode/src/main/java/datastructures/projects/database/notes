 public static void main(String[] args) throws JSQLParserException {

        DBTest dbtest = new DBTest();
        ResultSet resultSet1 = dbtest.execute("CREATE TABLE YCStudent"
                + "("
                + " BannerID int,"
                + " SSNum int,"
                + " FirstName varchar(255),"
                + " LastName varchar(255) NOT NULL,"
                + " GPA decimal(1,2) DEFAULT 0.01,"
                + " CurrentStudent boolean DEFAULT true,"
                + " Class varchar(255),"
                + " PRIMARY KEY (BannerID)"
                + ");");

        ResultSet resultSet2 = dbtest.execute("INSERT INTO YCStudent (FirstName, LastName, GPA, BannerID, CurrentStudent, SSNum) VALUES ('Noah','Potash', 4.0 ,80002, true, 40);");
        ResultSet resultSet3 = dbtest.execute("INSERT INTO YCStudent (FirstName, LastName, Class, GPA, BannerID, SSNum) VALUES ('Yonah', 'Taurog', 'Senior' ,3.9 ,80001 , 38);");
        ResultSet resultSet4 = dbtest.execute("INSERT INTO YCStudent (FirstName, LastName, GPA, BannerID) VALUES ('Basia', 'Ebel', 4.07, 8000909);");
        ResultSet resultSet5 = dbtest.execute("INSERT INTO YCStudent (FirstName, LastName, GPA, SSNum, BannerID) VALUES ('Noah', 'Baron', 4.0, 809, 04659234);");
        ResultSet resultSet12 = dbtest.execute("CREATE INDEX SSNum_Index on YCStudent (SSNum);");
       // ResultSet resultSet6 = dbtest.execute("UPDATE YCStudent SET Class = 'Super Senior' WHERE SSNum = 38;");
        ResultSet resultSet7 = dbtest.execute("SELECT * FROM YCStudent;");
        ResultSet resultSet8 = dbtest.execute("SELECT GPA, LastName FROM YCStudent;");
        ResultSet resultSet9 = dbtest.execute("SELECT * FROM YCStudent ORDER BY GPA DESC, FirstName ASC, SSNum DESC;");
        ResultSet resultSet10 = dbtest.execute("SELECT COUNT (DISTINCT FirstName) FROM YCStudent;");
        ResultSet resultSet11 = dbtest.execute("SELECT SUM (GPA) FROM YCStudent;");
        ResultSet resultSet17 = dbtest.execute("SELECT AVG (GPA) FROM YCStudent;");
        ResultSet resultSet18 = dbtest.execute("SELECT MAX (BannerID) FROM YCStudent;");
       // ResultSet resultSet13 = dbtest.execute("DELETE FROM YCStudent WHERE GPA = 4.0");
        ResultSet resultSet14 = dbtest.execute("SELECT FirstName, LastName, SSNum from YCStudent WHERE GPA = 3.9 OR FirstName = 'Noah' AND SSNum > 37;");
        ResultSet resultSet6 = dbtest.execute("UPDATE YCStudent SET Class = 'Super Senior' WHERE SSNum < 809;");
        dbtest.database.getTable("YCStudent").printWholeTable(); //just for convenience to see the table
        ResultSet resultSet15 = dbtest.execute("DELETE FROM YCStudent WHERE GPA < 4.0 AND FirstName = 'Yonah'");
        dbtest.database.getTable("YCStudent").printWholeTable();


    }