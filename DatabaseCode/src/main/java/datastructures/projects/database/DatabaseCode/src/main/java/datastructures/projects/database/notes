CREATE TABLE YCStudent( BannerID int, SSNum int, FirstName varchar(255), LastName varchar(255) NOT NULL, GPA decimal(1,2) DEFAULT 0.01, CurrentStudent boolean DEFAULT true, Class varchar(255), PRIMARY KEY (BannerID) );

INSERT INTO YCStudent (FirstName, LastName, GPA, BannerID, CurrentStudent, SSNum) VALUES ('Noah','Potash', 4.0 ,80002, true, 40);

